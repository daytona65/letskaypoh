{
    "version": 2,
    "builds": [
      {
        "src": "server/server.py",
        "use": "@vercel/python"
      },
      {
        "src": "letskaypoh/package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "letskaypoh/build" }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "server/server.py"
      },
      {
        "src": "/(.*)",
        "dest": "letskaypoh/$1"
      }
    ]
  }